<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <!-- Task Default Stages -->
    <record id="project_task_new" model="project.task.type">
        <field name="sequence">10</field>
        <field name="name">New</field>
        <field name="case_default" eval="True"/>
    </record>
    <record id="project_task_In Progress" model="project.task.type">
        <field name="sequence">20</field>
        <field name="name">In Progress</field>
        <field name="case_default" eval="True"/>
    </record>
    <record id="project_task_testing" model="project.task.type">
        <field name="sequence">30</field>
        <field name="name">Testing</field>
        <field name="case_default" eval="True"/>
    </record>
    <record id="project_task_reviews" model="project.task.type">
        <field name="sequence">40</field>
        <field name="name">Reviews</field>
        <field name="case_default" eval="True"/>
    </record>
    <record id="project_task_done" model="project.task.type">
        <field name="sequence">50</field>
        <field name="name">Done</field>
        <field name="case_default" eval="True"/>
    </record>
</odoo>
